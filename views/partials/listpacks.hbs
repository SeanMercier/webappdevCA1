{{#each packs}}
  <section class="ui inverted stacked segment">
    <h2 class="ui header" id="dashboard">
      {{title}}
    </h2>
    <p id="idparagraphs"> Total Duration: {{duration}} </p>
    <a href="/pack/{{id}}" class="ui large icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/removepack/{{id}}" class="ui large icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}